# Set compression
Compression yes

# Specify forwarding options
ForwardAgent yes
ForwardX11 no
ForwardX11Trusted yes

# Fix for OSX Sierra no longer saving ssh keys in keychain
AddKeysToAgent yes
UseKeychain yes

# Set up session keep alive settings
ServerAliveInterval 100
ServerAliveCountMax 2

Host bitbucket.org
    User git
    IdentityFile ~/.ssh/bitbucket_rsa
    IdentitiesOnly yes

Host github.com
    User git
    IdentityFile ~/.ssh/github_rsa
    IdentitiesOnly yes

Host gitlab.com
    User git
    IdentityFile ~/.ssh/gitlab_rsa
    IdentitiesOnly yes

Host ssh.gitlab.aws.dev
    User git
    IdentityFile ~/.ssh/aws_rsa

Host grocy
    Hostname grocy.skjerven.io
    User admin-local
    IdentityFile ~/.ssh/grocy_rsa
    IdentitiesOnly yes

Host ssh.dev.azure.com
    User git
    IdentityFile ~/.ssh/aws_rsa

Host homelab
    Hostname homelab.skjerven.io
    User home-admin
    IdentityFile ~/.ssh/homelab_rsa
    IdentitiesOnly yes

