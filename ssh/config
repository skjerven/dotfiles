# Set compression
Compression yes

# Specify forwarding options
ForwardAgent yes
ForwardX11 no
ForwardX11Trusted yes

# Fix for OSX Sierra no longer saving ssh keys in keychain
AddKeysToAgent yes
UseKeychain yes

# Set up session keep alive settings
ServerAliveInterval 100
ServerAliveCountMax 2

Host bitbucket.org
    User git
    IdentityFile ~/.ssh/bitbucket_rsa
    IdentitiesOnly yes

Host github.com
    User git
    IdentityFile ~/.ssh/github_rsa
    IdentitiesOnly yes

Host euler nas
    Hostname euler.home.skjerven.io
    User nas-admin
    Port 2022
    IdentityFile ~/.ssh/euler_rsa
    IdentitiesOnly yes

Host klein
    Hostname klein.home.skjerven.io
    User nas-admin
    Port 2022
    IdentityFile ~/.ssh/klein_rsa
    IdentitiesOnly yes

Host newton kube
    Hostname newton.home.skjerven.io
    User kube-admin
    IdentityFile ~/.ssh/newton_rsa
    IdentitiesOnly yes

Host pihole
    Hostname pi.hole
    User pihole
    IdentityFile ~/.ssh/pihole_rsa
    IdentitiesOnly yes
